{   
    "manifest_version": 2,
    "name": "Extensions for IXC",
    "description": "Extensions to assist in the insertion of the equipment series in the IXC",
    "version": "1.0",
    "icons":{
        "16": "images/icon_128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://assinante.nmultifibra.com.br/adm.php"],
            "js": ["content.js"],
            "css": ["resources/style.css"]
        }
    ],
    "browser_action": {
        "default_title": "First Teste IXC",
        "default_popup": "./popup/index.html"
    },
    "permissions": ["tabs"] 
}
