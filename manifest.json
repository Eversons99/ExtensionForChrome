{   
    "manifest_version": 2,
    "name": "Test Extensions IXC",
    "description": "Extens√£o para editar os SNS dos equipamentos e salva-los",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "page": "background/background.html",
        "persistent": false
    },
    "options_ui": {
        "open_in_tab": true,
        "page": "background/background.html" 
    },  
    "browser_action": {
        "default_title": "First Teste IXC",
        "default_popup": "./popup/index.html"
    },
    "permissions": ["tabs", "storage"]
}
